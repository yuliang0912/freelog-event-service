{
  "name": "freelog-event-service",
  "version": "1.0.0",
  "description": "飞致网络事件服务!",
  "private": true,
  "dependencies": {
    "async": "^3.2.0",
    "egg-freelog-base": "^3.1.4",
    "egg-scripts": "^2.10.0",
    "jsonschema": "^1.2.6",
    "kafkajs": "^1.15.0",
    "midway": "^1.20.2",
    "moment": "^2.29.1",
    "mongoose": "^5.12.2",
    "node-schedule": "^2.0.0",
    "patrun": "^4.0.0",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.56",
    "cross-env": "^6.0.0",
    "egg-ci": "^1.18.0",
    "midway-bin": "^1.20.3",
    "midway-mock": "^1.20.3",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "tslint": "^6.1.3",
    "tslint-midway-contrib": "^1.20.1",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "scripts": {
    "start": "egg-scripts start --title=freelog-event-service --framework=midway --ts",
    "stop": "egg-scripts stop --title=freelog-event-service",
    "start_build": "npm run build && cross-env NODE_ENV=development midway-bin dev",
    "clean": "midway-bin clean",
    "dev": "cross-env NODE_ENV=local port=7010 midway-bin dev --ts",
    "debug": "cross-env NODE_ENV=local midway-bin debug --ts",
    "test": "npm run lint && midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "tslint --fix -p tsconfig.json -t stylish",
    "ci": "npm run cov",
    "build": "midway-bin build -c",
    "autod": "midway-bin autod"
  },
  "ci": {
    "version": "10"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "yuliang",
  "license": "MIT"
}
